#!/home/apps/software/Python/3.6.1-IGB-gcc-4.9.4/bin/python
# EASY-INSTALL-ENTRY-SCRIPT: 'weblogo==3.7.4','console_scripts','weblogo'
__requires__ = 'weblogo==3.7.4'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('weblogo==3.7.4', 'console_scripts', 'weblogo')()
    )
